			geom_bar(aes(fill=variable),#
			position ="dodge",#
			stat="identity",#
			width=.25) +#
			theme(panel.grid.major = element_blank(),#
			panel.grid.minor = element_blank(),#
			panel.background = element_rect(fill="white"),#
			plot.background = element_blank(),#
			axis.ticks = element_blank(),#
