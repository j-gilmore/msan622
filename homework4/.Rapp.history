			plot_list<- p#
			return(plot_list)#
		}#
	}#
	if(cloudtype=="comparison") {#
		comparison.cloud(corp_m, max.words=200,#
		random.order=FALSE,colors=palette,#
		title.size = .001,#
		main="Differences Between Inferno, Purgatory and Paradise")#
